{
    "build": {
      "content": [
        {
          "src": "./",
          "files": [
            "**/*.md",
            "**/*.yml"
          ],
          "exclude": [
            "**/obj/**",
            "**/includes/**",
            "**/docfx.json"
          ]
        }
      ],
      "resource": [
        {
          "src": "./",
          "files": [
            "**/*.png",
            "**/*.jpg",
            "**/*.gif",
            "**/*.svg"
          ],
          "exclude": [
            "**/obj/**",
            "**/includes/**",
            "**/docfx.json"
          ]
        }
      ],
      "overwrite": [],
      "externalReference": [],
      "globalMetadata": {
         "uhfHeaderId": "MSDocsHeader-VisualStudio",
        "breadcrumb_path": "/visualstudio/gamedev/breadcrumb/toc.json",
        "extendBreadcrumb": true,
        "feedback_system": "Standard",
        "zone_pivot_group_filename": "gamedev/zone-pivot-groups.json"
      },
      "fileMetadata": {},
      "template": [],
      "dest": "gamedev",
      "markdownEngineName": "markdig"
    }
  }
